call plug#begin()
Plug 'dracula/vim', { 'as': 'dracula' }
call plug#end()
colorscheme dracula
"turn tabs into spaces
set expandtab 
"set tab to four spaces
set tabstop=4
"change indent width
set shiftwidth=4
"backspace deletes tab
set softtabstop=4
"add number lines
set number
"auto indent
set autoindent


